generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model EnergyBill {
  id               String   @id @default(uuid())
  clientNumber     String
  referenceMonth   String
  electricityQty   Float
  electricityValue Float
  sceeQty          Float
  sceeValue        Float
  compensatedQty   Float
  compensatedValue Float
  publicLighting   Float
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
